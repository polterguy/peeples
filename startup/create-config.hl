
/*
 * Creates the default config file, unless it already exists.
 */
if
  fetch:x:/0/0?value
    file-exists:@PEEPLES/configuration/config.hl
  not
  .file
    tix:int:5
    hash
  p5.crypto.hash.create-sha256:x:/@.file/*/tix?value.string
  set:x:/@.file/*/hash?value
    src:x:/@p5.crypto.hash.create-sha256?value
  lambda2hyper:x:/@.file/*
  save-file:@PEEPLES/configuration/config.hl
    src:x:/@lambda2hyper?value
